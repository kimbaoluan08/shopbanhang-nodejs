    <script>
  $(document).ready(function() {
        $('button[name="delete-contact"]').on('click', function(e) {
            var $form = $(this).closest('form');
            e.preventDefault();
            $('#delete-confirm').modal({
                    backdrop: 'static',
                    keyboard: false
                })
                .one('click', '#delete', function() {
                    $form.trigger('submit');
                });
        });
  });
  </script>
  
